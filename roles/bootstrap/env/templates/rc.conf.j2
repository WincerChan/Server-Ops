hostname="entropy"
ifconfig_em0="inet {{ ipv4_addr }} netmask 0xffffff00"
ifconfig_em0_ipv6="inet6 {{ ipv6_addr }} prefixlen 48"
ipv6_defaultrouter="{{ ipv6_router }}"
defaultrouter="{{ ipv4_router }}"
sshd_enable="YES"
# Set dumpdev to "AUTO" to enable crash dumps, "NO" to disable
dumpdev="AUTO"
zfs_enable="YES"
kld_list="tcp_bbr tcp_rack"
cloned_interfaces="lo1"
ifconfig_lo1="inet 10.5.0.0/24"